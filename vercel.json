{
    "version": 2,
    "builds": [
        {
            "src": "lib/index.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "45mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "lib/index.js"
        }
    ],
    "functions": {
        "lib/index.js": {
            "maxDuration": 30,
            "memory": 1024
        }
    }
}
